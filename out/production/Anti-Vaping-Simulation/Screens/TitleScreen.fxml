            <Button fx:id="helpButton" mnemonicParsing="false" scaleX="2.0" scaleY="2.0" text="Help" translateX="50.0" translateY="10.0" GridPane.columnIndex="5" GridPane.halignment="LEFT" GridPane.rowIndex="5" GridPane.valignment="TOP">
